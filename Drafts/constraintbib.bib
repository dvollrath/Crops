	Title = {{How Deep Are the Roots of Economic Development?}},
	Url = {https://ideas.repec.org/a/aea/jeclit/v51y2013i2p325-69.html},
	Volume = {51},
	Year = 2013,
	Bdsk-File-1 = {},
